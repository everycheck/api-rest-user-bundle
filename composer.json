{
    "name": "everycheck/api-rest-user-bundle",
    "license": "MIT",
    "type": "project",
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.5.9",
        "ext-pdo_mysql": "*",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/orm": "^2.5",
        "everycheck/api-rest-utils": "^1.0.1",
        "everycheck/php-simple-acl": "^1.0",
        "firebase/php-jwt": "^5.0",
        "incenteev/composer-parameter-handler": "^2.0",
        "ircmaxell/random-lib": "^1.2",
        "jms/serializer-bundle": "^1.0|^2.0",
        "ramsey/uuid-doctrine": "^1.5",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^5.0",
        "symfony/monolog-bundle": "^3.1",
        "symfony/polyfill-apcu": "^1.0",
        "symfony/swiftmailer-bundle": "^2.6",
        "symfony/symfony": "3.4.*"
    },
    "require-dev": {
        "sensio/generator-bundle": "^3.0",
        "symfony/phpunit-bridge": "^3.0",
        "everycheck/test-api-rest-bundle": "^1.2"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ],
        "reset": [
            "bin/console doctrine:database:drop --force --ansi",
            "bin/console doctrine:database:create --ansi",
            "bin/console doctrine:schema:create --ansi",
            "bin/console doctrine:fixtures:load --append --ansi"
        ],
        "test":[
            "php -d memory_limit=-1 vendor/bin/phpunit -c . --colors=always"
        ],
        "run-test": [
            "bin/console test:database:build-all -v",
            "@test"
        ]
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": null
    },
    "config": {
        "platform": {
            "php": "7.1"
        },
        "sort-packages": true
    }
}
