{
    "name": "everycheck/user-api-rest-bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "require": {
        "php": ">=5.5.9",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/orm": "^2.5",
        "everycheck/api-rest-utils": "^1.0.1",
        "firebase/php-jwt": "^5.0",
        "incenteev/composer-parameter-handler": "^2.0",
        "ircmaxell/random-lib": "^1.2",
        "jms/serializer-bundle": "^1.0|^2.0",
        "ramsey/uuid-doctrine": "^1.5"
    },
    "require-dev": {
        "sensio/generator-bundle": "^3.0",
        "symfony/phpunit-bridge": "^3.0",
        "everycheck/test-api-rest-bundle": "^1.2",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^5.0",
        "symfony/monolog-bundle": "^3.1",
        "symfony/polyfill-apcu": "^1.0",
        "symfony/symfony": "3.4.*",
        "everycheck/php-simple-acl": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "EveryCheck\\UserApiRestBundle\\": "src/"
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "EveryCheck\\UserApiRestBundle\\Tests\\": "Tests"
        },
        "files" : ["Tests/testProject/app/AppKernel.php"]
    }
}
